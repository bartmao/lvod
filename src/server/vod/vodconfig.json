    "outputDir": "C:/Users/bmao002/Desktop/Projects/lvod/resources",
    "cmd": {
        "ffmpeg": "ffmpeg",
        "mp4box": "MP4Box"
    },
    "filters": [
        ".mkv",
        ".mp4"
    ],
    "ffmpegtrans": "-y -i ${input} -map 0:0 -c:v libx264 -preset ultrafast -x264opts keyint=150:min-keyint=150:no-scenecut -b:v 400k ${videoOutput} -map 0:1 -c:a aac -strict -2 -b:a 128k ${audioOutput}",
    "mp4boxpkg4v": "-dash 10000 -frag 10000 -rap -segment-name v_ 1v.mp4#video",
    "mp4boxpkg4a": "-dash 10000 -frag 10000 -rap -segment-name a_ 1a.mp4#audio"
}